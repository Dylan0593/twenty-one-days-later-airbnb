<div class="cards-home">
  <h1>SuperPowers!</h1>

  <h2>List of superpowers</h2>
    <div class="cards">

    <% @superpowers.each do |superpower| %>
     <div class="card">

            <%= link_to  superpower_path(superpower) do %>
            <%= cl_image_tag superpower.photo.key, height: 300, width: 200, crop: :fill, alt: superpower.name %>
            <% end %>

    </div>
    <%end%>
</div>
</div>
